image/jpeg; eog %s
image/pjpeg; eog %s
application/msword; antiword %s; copiousoutput;
#application/msword; /usr/local/OpenOffice.org/program/soffice %s
#application/vnd.ms-excel; /usr/local/OpenOffice.org/program/soffice %s
